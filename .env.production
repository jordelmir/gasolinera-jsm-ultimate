# Database Configuration
POSTGRES_DB=gasolinera_jsm_prod
POSTGRES_USER=gasolinera_user
POSTGRES_PASSWORD=your_secure_postgres_password_here

# Redis Configuration
REDIS_PASSWORD=your_secure_redis_password_here

# RabbitMQ Configuration
RABBITMQ_USER=gasolinera_rabbitmq
RABBITMQ_PASSWORD=your_secure_rabbitmq_password_here

# Vault Configuration
VAULT_ROOT_TOKEN=your_secure_vault_token_here

# API Gateway
API_GATEWAY_URL=https://api.gasolinera-jsm.com

# JWT Secrets
JWT_SECRET=your_super_secure_jwt_secret_key_here_minimum_32_chars
JWT_REFRESH_SECRET=your_super_secure_jwt_refresh_secret_key_here_minimum_32_chars

# QR Code Security
QR_SIGNATURE_SECRET=your_secure_qr_signature_secret_here

# Monitoring
GRAFANA_PASSWORD=your_secure_grafana_password_here

# External Services
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=noreply@gasolinera-jsm.com
SMTP_PASSWORD=your_email_password_here

# Push Notifications
FCM_SERVER_KEY=your_firebase_server_key_here
APNS_KEY_ID=your_apple_push_key_id_here
APNS_TEAM_ID=your_apple_team_id_here

# File Storage
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_S3_BUCKET=gasolinera-jsm-assets
AWS_REGION=us-east-1

# Domain Configuration
DOMAIN=gasolinera-jsm.com
SSL_EMAIL=admin@gasolinera-jsm.com

# Backup Configuration
BACKUP_S3_BUCKET=gasolinera-jsm-backups
BACKUP_RETENTION_DAYS=30

# Rate Limiting
RATE_LIMIT_REQUESTS_PER_MINUTE=100
RATE_LIMIT_BURST=200

# Security
CORS_ALLOWED_ORIGINS=https://gasolinera-jsm.com,https://www.gasolinera-jsm.com,https://admin.gasolinera-jsm.com

# Analytics
GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
MIXPANEL_TOKEN=your_mixpanel_token_here

# Feature Flags
ENABLE_WEEKLY_RAFFLE=true
ENABLE_ANNUAL_RAFFLE=true
ENABLE_PUSH_NOTIFICATIONS=true
ENABLE_EMAIL_NOTIFICATIONS=true
MAX_AD_SEQUENCE_STEPS=10
MAX_QR_EXPIRY_HOURS=24